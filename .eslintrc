{
  "extends": "airbnb",
  "rules": {
    "camelcase": [2, {
        "properties": "always"
      }
    ],
    "comma-dangle": [0],
    "func-names": [0],
    "no-path-concat": [2],
    "no-var": [1],
    "prefer-const": [2],
    "space-before-function-paren": [2, {
        "anonymous": "always",
        "named": "never"
      }
    ]
  }
}
